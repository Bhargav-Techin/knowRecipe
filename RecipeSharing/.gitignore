# Node.js dependencies
node_modules/
package-lock.json
yarn.lock

# Angular build output
dist/
out-tsc/

# IDE & Editor files (Optional but recommended)
.vscode/
.idea/
*.iml

# Environment files (to prevent leaking secrets)
src/environments/environment
src/environments/environment.development

# If using dynamic runtime config (prevent committing actual values)
src/assets/config.json

# Logs and system files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# MacOS specific files
.DS_Store

# Temporary files
temp/
tmp/

# Angular cache files
.angular/
